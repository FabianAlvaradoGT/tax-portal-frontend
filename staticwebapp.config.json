{
  "routes": [
    {
      "route": "/*",
      "statusCode": 200,
      "headers": {
        "Cache-Control": "no-store"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
